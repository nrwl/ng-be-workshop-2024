{
  "$schema": "https://json-schema.org/schema",
  "$id": "UtilLib",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    },
    "directory": {
      "type": "string",
      "description": "Provide the directory",
      "enum": [
        "movies",
        "api",
        "shared"
      ],
      "x-prompt": {
        "message": "What directory would you like to use?",
        "type": "list",
        "items": [
          {
            "value": "movies",
            "label": "movies"
          },
          {
            "value": "api",
            "label": "api"
          },
          {
            "value": "shared",
            "label": "shared"
          }
        ]
      }
    }
  }
}